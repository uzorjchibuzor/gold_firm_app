<div>
  <h1 class="font-bold text-4xl">Examinations#index</h1>
</div>
    <table class="w-full text-sm text-left rtl:text-right text-gray-500 dark:text-gray-400">
      <thead class="text-xs text-gray-700 uppercase bg-gray-50 dark-bg-gray-700 dark:text-gray-400">
        <tr>
          <th scope="col" class="px-6 py-3">S/N</th>
          <th scope="col" class="px-6 py-3">Student Name</th>
          <th scope="col" class="px-6 py-3">Session</th>
          <th scope="col" class="px-6 py-3">Term</th>
          <th scope="col" class="px-6 py-3">Subject</th>
          <th scope="col" class="px-6 py-3">Exam Type</th>
          <th scope="col" class="px-6 py-3">Score</th>
          <th scope="col" class="px-6 py-3">Entered By</th>
        </tr>
      </thead>
       <tbody>
        <% @examinations.each_with_index do |examination, index| %>
          <tr class="odd:bg:white odd:dark:bg-gray-900 even:bg-gray-50 even:dark:bg-gray-800 border-b dark:border-gray-700 border-gray-200">
            <th scope="row" class="px-6 py-4"><%= index + 1 %></th>
            <th class="px-6 py-4"><%= examination.user.full_name %></th>
            <th class="px-6 py-4"><%= examination.grade_level.school_year.title %></th>
            <th class="px-6 py-4"><%= examination.school_term.title %></th>
            <th class="px-6 py-4"> <%=examination.subject.title %> </th>
            <th class="px-6 py-4"> <%= examination.exam_type.humanize %> </th>
            <th class="px-6 py-4"> <%= examination.score %> </th>
            <th class="px-6 py-4"> <%= examination.creator.full_name %> </th>
            <th class="px-6 py-4">
          </tr>
        <% end %>
      </tbody>
    </table>